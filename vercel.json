{
  "version": 2,
  "builds": [
    {
      "src": "zyloZyraxBackend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "zyloZyraxBackend/wsgi.py": {
      "runtime": "python3.9",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "zyloZyraxBackend/wsgi.py"
    }
  ]
}
